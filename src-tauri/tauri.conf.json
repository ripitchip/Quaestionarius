{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "queastonarius",
  "version": "0.1.0",
  "identifier": "com.vscode.queastonarius",
  "build": {
    "beforeDevCommand": "bash src-tauri/build_python.sh && pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bash src-tauri/build_python.sh && pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Quaestionarius - Google Forms Auth",
        "width": 900,
        "height": 700
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "externalBin": [
      "binaries/google_auth",
      "binaries/json_processor"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}